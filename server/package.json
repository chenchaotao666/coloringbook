{
  "name": "coloring-book-server",
  "version": "1.0.0",
  "description": "Coloring Book API Server with PostgreSQL",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "debug": "nodemon --inspect src/server.js",
